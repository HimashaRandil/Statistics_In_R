ggplot(mtcars, aes(x=wt, y= mpg))+geom_point()+ geom_smooth(method=lm, se=FALSE)
ggplot(mtcars, aes(x=wt, y= mpg))+geom_point(aes(color=factor(gear)), size=2)+ geom_smooth(method=lm, se=FALSE)
